    "babel-loader": "^8.1.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "glob": "^7.1.6",
    "regenerator-runtime": "^0.13.5",
